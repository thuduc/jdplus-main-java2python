<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE filesystem PUBLIC "-//NetBeans//DTD Filesystem 1.0//EN" "http://www.netbeans.org/dtds/filesystem-1_0.dtd">
<!--
Examples: https://github.com/apache/netbeans/blob/master/platform/core.ui/src/org/netbeans/core/ui/resources/layer.xml
-->
<filesystem>

    <folder name="Menu">
        <folder name="File">
            <file name="Separator2.instance_hidden"/>
            <file name="Separator3.instance_hidden"/>
            <file name="org-netbeans-modules-print-action-PageSetupAction.shadow_hidden"/>
            <file name="org-netbeans-modules-print-action-PrintAction.shadow_hidden"/>
            <file name="org-openide-actions-SaveAction.shadow_hidden"/>
            <file name="org-openide-actions-SaveAllAction.shadow_hidden"/>
            <file name="org-openide-actions-SaveAsAction.shadow_hidden"/>
        </folder>
        <file name="Edit_hidden"/>
        <file name="GoTo_hidden"/>
        <folder name="Tools">
            <file name="org-netbeans-modules-templates-actions-TemplatesAction.shadow_hidden"/>
        </folder>
        <folder name="Window">
            <file name="org-netbeans-modules-favorites-View.shadow_hidden"/>
            <folder name="Web_hidden"/>
            <folder name="Tools_hidden"/>
            <file name="org-netbeans-core-windows-actions-GlobalPropertiesAction.shadow">
                <attr name="originalFile" stringvalue="Actions/Window/org-netbeans-core-windows-actions-GlobalPropertiesAction.instance"/>
                <attr name="position" intvalue="200"/>
            </file>
        </folder>
        <folder name="Help">
            <file name="org-netbeans-modules-autoupdate-ui-actions-CheckForUpdatesAction.shadow_hidden"/>
        </folder>
    </folder>

    <folder name="Toolbars">
        <folder name="File_hidden"/>
        <folder name="Clipboard_hidden"/>
        <folder name="UndoRedo_hidden"/>
        <folder name="Memory">
            <file name="org-netbeans-modules-masterfs-ui-suspend-PauseAction.shadow_hidden"/>
            <file name="org-netbeans-modules-profiler-actions-SelfSamplerAction.shadow_hidden"/>
        </folder>
    </folder>

    <folder name="OptionsDialog">
        <file name="Advanced_hidden"/>
    </folder>

    <file name="OptionsExport_hidden"/>

    <folder name="Windows2">
        <folder name="Modes">
            <file name="tsnavigator.wsmode" url="./layout/tsnavigatorWsmode.xml"/>
        </folder>
    </folder>

</filesystem>
